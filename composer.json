{
    "name": "affiliated-writer/api",
    "type": "project",
    "require": {
        "php": "^8.1",
        "firebase/php-jwt": "6.0",
        "illuminate/database": "^12.0",
        "monolog/monolog": "^3.7",
        "php-di/php-di": "^7.1",
        "selective/basepath": "^2.3",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "vlucas/phpdotenv": "^5.6",
        "tuupola/cors-middleware": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}